{
  "plugin": "org.smartide.plugin.java",
  "run": "java Main.java",
  "gui": false,
  "intelligence": {
    ".json": {
      "enabled": true,
      "run": "vscode-json-language-server --stdio"
    },
    ".java": {
      "enabled": false,
      "run": "jdtls"
    },
    ".properties": {
      "enabled": true,
      "run": "lemminx"
    },
    ".yml": {
      "enabled": false,
      "run": "yaml-language-server --stdio"
    },
    ".gradle": {
      "enabled": true,
      "run": "groovy-language-server"
    },
    ".xml": {
      "enabled": false,
      "run": "lemminx"
    }
  }
}